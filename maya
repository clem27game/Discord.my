
#!/bin/bash

# Script global maya pour Discord.my
if [ "$1" = "Discord.my-allume" ]; then
    if [ -z "$2" ]; then
        echo "‚ùå Usage: maya Discord.my-allume <fichier.my>"
        exit 1
    fi
    
    node ./Discord.my/discord-my.js "$2"
elif [ "$1" = "version" ]; then
    echo "Discord.my v1.0.0 - Version Discord du langage Maya"
elif [ "$1" = "help" ]; then
    echo "
ü§ñ Discord.my - Commandes disponibles:

maya Discord.my-allume <fichier.my>    # D√©marrer un bot Discord
maya version                           # Afficher la version
maya help                             # Afficher cette aide
    "
else
    echo "‚ùå Commande inconnue. Utilisez 'maya help' pour voir les commandes disponibles."
fi
